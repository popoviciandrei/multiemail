{
  "name": "popovici/multiemail",
  "type": "magento-module",
  "description": "Now you can have transactional email templates per theme. Useful for multi store shops that want to implement different designs based on the theme layout.",
  "license": [
    "proprietary"
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "map": [
      ["app/code/community/Popovici/Multiemail","app/code/community/Popovici/Multiemail"],
      ["app/design/adminhtml/default/default/layout/popovici/multiemail.xml","app/design/adminhtml/default/default/layout/popovici/multiemail.xml"],
      ["app/etc/modules/Popovici_Multiemail.xml","app/etc/modules/Popovici_Multiemail.xml"]
    ]
  }
}